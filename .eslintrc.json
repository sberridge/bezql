{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "extends": [
        "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "double"],
        "@typescript-eslint/no-explicit-any": ["off"]
    },
    "settings": {
    }
}
